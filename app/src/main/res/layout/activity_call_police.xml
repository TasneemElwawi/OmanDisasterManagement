<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activities.CallPolice">

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/roboto_regular"
        android:text="Call Royal Oman Police."
        android:textColor="@color/green_color"
        android:textSize="@dimen/_11sdp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/police"
        app:layout_constraintStart_toStartOf="@+id/police"
        app:layout_constraintTop_toBottomOf="@+id/police" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/roboto_regular"
        android:text="Call royal hospital."
        android:textColor="@color/green_color"
        android:textSize="@dimen/_11sdp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/hospital"
        app:layout_constraintStart_toStartOf="@+id/hospital"
        app:layout_constraintTop_toBottomOf="@+id/hospital" />

    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="If you need help you can call "
        android:textColor="@color/green_color"
        android:textSize="@dimen/_20sdp"
        android:textStyle="normal"
        android:fontFamily="@font/roboto_bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Call Ambulance we save life's fast"
        android:textColor="@color/RED_color"
        android:textSize="@dimen/_15sdp"
        android:textStyle="normal"
        android:fontFamily="@font/roboto_rlack"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <ImageButton
        android:id="@+id/hospital"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        app:layout_constraintStart_toStartOf="@+id/amb"
        app:layout_constraintTop_toBottomOf="@+id/amb"
        app:srcCompat="@drawable/hospital" />

    <ImageButton
        android:id="@+id/police"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        app:layout_constraintStart_toStartOf="@+id/hospital"
        app:layout_constraintTop_toBottomOf="@+id/hospital"
        app:srcCompat="@drawable/police" />

    <ImageButton
        android:id="@+id/amb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:srcCompat="@drawable/ambulance" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/roboto_regular"
        android:text="CALL Public Authority for Civil Defence and Ambulance."
        android:textColor="@color/green_color"
        android:textSize="@dimen/_11sdp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/amb"
        app:layout_constraintStart_toStartOf="@+id/amb"
        app:layout_constraintTop_toBottomOf="@+id/amb" />

</androidx.constraintlayout.widget.ConstraintLayout>